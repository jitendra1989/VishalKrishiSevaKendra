.row
  .col-md-12
    %h2 Permission
  .col-md-10.col-md-offset-1
    .profile-box
      %br
      %table
        %tr
          %th Name
          %th Subject
          %th Action
          %th Description
        - @permissions.each do |permission|
          %tr
            %td= permission.name
            %td= permission.subject_class
            %td= permission.action
            %td= permission.description
