.row
	.col-md-12
		%h2 Admin
	.col-md-10.col-md-offset-1
		.profile-box
			.text-right
				=link_to 'Create Admin', new_admin_user_path
			%br
			%table
				%tr
					%th Name
					%th Outlet
					%th Email
					%th Contact No
					%th Action
				- @users.each do |user|
					%tr
						%td= user.name
						%td #
						%td= user.email
						%td= user.phone
						%td
							= link_to edit_admin_user_path(user) do
								%span.glyphicon.glyphicon-pencil
