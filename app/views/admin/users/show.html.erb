<div class="row">
	<div class="col-md-12">
		<h2>Profile</h2>
	</div>
	<div class="col-md-12">
		<div class="profile-box">
			<div class="text-right">
				<%= link_to 'Edit Profile', edit_admin_user_path(@user) %>
			</div>
			<p>
				<strong class="title">Name:</strong>
				<%= @user.name %>
			</p>
			<p>
				<strong class="title">Username:</strong>
				<%= @user.username %>
			</p>
			<p>
				<strong class="title">Email:</strong>
				<%= @user.email %>
			</p>
			<p>
				<strong class="title">Contact No:</strong>
				<%= @user.phone %>
			</p>
			<p>
				<strong class="title">Address:</strong>
				<%= @user.address %>
			</p>
			<p>
				<strong class="title">Pincode:</strong>
				<%= @user.pincode %>
			</p>
			<p>
				<strong class="title">City:</strong>
				<%= @user.city %>
			</p>
			<p>
				<strong class="title">State:</strong>
				<%= @user.state %>
			</p>
			<p>
				<strong class="title">Country:</strong>
				<%= @user.country %>
			</p>
		</div>
	</div>
</div>
