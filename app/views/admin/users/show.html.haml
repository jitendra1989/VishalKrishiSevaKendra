.row
	.col-md-12
		%h2 Profile
	.col-md-6.col-md-offset-3
		.profile-box
			.text-right
				=link_to 'Edit Profile', edit_admin_user_path(@user)
			%p
				%strong.title Name:
				= @user.name
			%p
				%strong.title Username:
				= @user.username
			%p
				%strong.title Email:
				= @user.email
			%p
				%strong.title Contact No:
				= @user.phone